#!/home/leo/src/github.com/addok/addok/addok/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'addok','console_scripts','addok'
__requires__ = 'addok'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('addok', 'console_scripts', 'addok')()
    )
